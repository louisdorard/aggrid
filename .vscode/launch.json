{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "DKU_CURRENT_PROJECT_KEY": "COMPANY_RECONCILIATION",
                "ORIGINAL_DATASET": "matches_uncertain"
            },
        },
        {
            "name": "Dash",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_DEBUG": "1",
                "DKU_CURRENT_PROJECT_KEY": "COMPANY_RECONCILIATION",
                "ORIGINAL_DATASET": "matches_uncertain"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true,
            "justMyCode": true
        }
    ]
}